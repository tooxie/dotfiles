*background: #555753
*boldFont: inconsolata
*faceName: Inconsolata
*faceSize: 16
*fastScroll: 1
*font: xft:inconsolata:pixelsize=16:antialias=true
*foreground: #DCDCDC
*jumpScroll: 1
*keysym*Control-Shift-c: perl:clipboard:copy
*keysym*Control-Shift-v: perl:clipboard:paste
*saveLines: 1000000
*scrollBar: 0
*scrollKey: 1
*scrollTtyKeypress: 1
*scrollTtyOutput: 0
*scrollWithBuffer: 1
*utf8: 1
*VT100*font: xft:inconsolata:pixelsize=16:antialias=true
*vt100*translations: #override \
    Shift Ctrl <Key> C: copy-selection(CLIPBOARD) \n\
    Shift Ctrl <Key> V: insert-selection(CLIPBOARD)
